package 문제모음1;

import java.util.Scanner;

public class exam {

	public static void main(String[] args) {

		System.out.print("숫자를 입력하십시오 :");
		// 키보드 숫자값 받음
		int n = (new Scanner(System.in)).nextInt();
		for (int i = 0; i < 8; i++) {
		int[] arrayi = new int[n];
		
		System.out.print(arrayi.notify());
		}
	}
}
